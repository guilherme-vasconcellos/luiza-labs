{
  "name": "node-express",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "rimraf dist && babel src --out-dir dist",
    "build": "docker-compose build",
    "postbuild": "rimraf dist",
    "dev": "nodemon --exec babel-node src/app.js -r node_modules/dotenv/config",
    "start": "docker-compose up",
    "test": "jest --detectOpenHandles --runInBand",
    "test:unit": "yarn test --testPathIgnorePatterns \"integration/.*\"",
    "test:integration": "yarn test --testPathIgnorePatterns \"unit/.*\""
  },
  "dependencies": {
    "@babel/runtime": "7.5.5",
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.3",
    "helmet": "3.19.0",
    "mongoose": "5.6.5",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "dotenv": "8.0.0",
    "dotenv-cli": "2.0.1",
    "eslint": "6.1.0",
    "eslint-config-standard": "13.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.13.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "rimraf": "2.6.3"
  }
}
