{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Node Babel",
            "program": "${workspaceFolder}/src/index.js",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "BABEL_ENV": "debug"
            },
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}